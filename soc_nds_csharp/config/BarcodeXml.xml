<?xml version="1.0" encoding="gb2312"?>
<root>
  <CurrentPrinter>TSC  TTP-344M Plus</CurrentPrinter>                 <!--选择当前打印机-->
  
  <tscOutPutPort>TSC  TTP-344M Plus</tscOutPutPort>  <!--TSC TTP-344M Plus-->
  <tscWidth>80</tscWidth>                           <!--卷标宽度-->
  <tscHeight>55</tscHeight>                         <!--卷标高度-->
  <tscPrintSpeed>4</tscPrintSpeed>                  <!--打印速度-->
  <tscDensity>8</tscDensity>                        <!--打印浓度-->
  <tscSensor>0</tscSensor>                          <!--感应器类别,不常用2222-->
  <tscVertical>2</tscVertical>                      <!--垂直间距单位mm，不常用2222-->
  <tscOffset>0</tscOffset>                          <!--偏移距离单位mm，不常用2222-->
  <tscX>200</tscX>                                  <!--条形码X方向起始点-->
  <tscY>150</tscY>                                  <!--条形码Y方向起始点-->
  <tscFontType>3</tscFontType>                      <!--文字类型，不常用2222-->
  <tscFontRotation>0</tscFontRotation>              <!--文字旋转角度，不常用22222-->
  <tscEncodeType>128</tscEncodeType>                <!--编码格式-->
  <tscBarcodeHeight>100</tscBarcodeHeight>          <!--条码高度，不常用22222-->
  <tscPrintCode>0</tscPrintCode>                    <!--是否打印系统码文（如果用自定义的码文，则不打印）0为不打印，1为打印-->
  <tscCodeInterval>100</tscCodeInterval>            <!--自定义码文与条形码之间间隔-->
  <tscFontMagnify1>1</tscFontMagnify1>              <!--设定文字X方向放大倍率,1-8-->
  <tscFontMagnify2>1</tscFontMagnify2>              <!--设定文字X方向放大倍率,1-8-->
  <tscBarCodeInterval>140</tscBarCodeInterval>      <!--条形码之间的间隔-->
  <tscRotate>0</tscRotate>                          <!--设定条形码旋转角度-->
  <tscBarNarrow>2</tscBarNarrow>                    <!--条形码窄bar 比例因子2222 要更换下-->
  <tscBarWide>2</tscBarWide>                        <!--条形码宽bar 比例因子，不常用2222-->
  <tscPrintLabelSetNum>1</tscPrintLabelSetNum>      <!--打印式数2222-->
  <tscPrintLabelCopeNum>1</tscPrintLabelCopeNum>    <!--打印份数2222-->

  <POSTEKOutPutPort>POSTEK C168 200s</POSTEKOutPutPort>    <!--博思得打印机-->
  <POSTEKPrintSpeed>3</POSTEKPrintSpeed>                   <!--打印速度，取值范围：0-6-->
  <POSTEKDensity>8</POSTEKDensity>                         <!--设置打印头发热温度,范围0-20缺省值10-->
  <POSTEKHeight>600</POSTEKHeight>                         <!--设置标签的高度（以dots为单位200DPI:1点＝1/8mm 300DPI:1点=1/12mm）-->
  <POSTEKInterval>200</POSTEKInterval>                     <!--设置定位间隙\黑线\穿孔的高度（以dots为单位200DPI:1点＝1/8mm 300DPI:1点=1/12mm）-->
  <POSTEKWidth>800</POSTEKWidth>                           <!--设置标签的宽度（以dots为单位200DPI:1点＝1/8mm 300DPI:1点=1/12mm）-->
  <POSTEKX>100</POSTEKX>                                   <!--条形码X坐标-->
  <POSTEKY>20</POSTEKY>                                    <!--条形码Y坐标-->
  <POSTEKBarRotation>0</POSTEKBarRotation>                 <!--条码打印方向-->
  <POSTEKEncodeType>1</POSTEKEncodeType>                   <!--条码类型-->
  <POSTEKBarNarrow>3</POSTEKBarNarrow>                     <!--条形码窄单元的宽度（以点dots为单位）-->
  <POSTEKBarWide>3</POSTEKBarWide>                         <!--条形码宽单元的宽度（以点dots为单位）-->
  <POSTEKBarcodeHeight>80</POSTEKBarcodeHeight>            <!--条形码高度-->
  <POSTEKPrintCode>N</POSTEKPrintCode>                     <!--打印码文（如果用自定义的码文，则不打印）0为不打印，1为打印-->
  <POSTEKBarCodeInterval>140</POSTEKBarCodeInterval>       <!--条形码之间的间距-->
  <POSTEKCodeInterval>100</POSTEKCodeInterval>             <!--自定义码文与标签间隔-->
  <POSTEKFontHeight>40</POSTEKFontHeight>                  <!--字型高度-->
  <POSTEKFontWidth>40</POSTEKFontWidth>                    <!--字型宽度-->
  <POSTEKFontType></POSTEKFontType>                        <!--字型名称-->
  <POSTEKFontAlign></POSTEKFontAlign>                      <!--字体旋转角度以及对齐方式-->
  <POSTEKFontWeight></POSTEKFontWeight>                    <!--字体粗细-->
  <POSTEKPrintLabelSetNum>1</POSTEKPrintLabelSetNum>       <!--打印标签-->
  <POSTEKPrintLabelCopeNum>1</POSTEKPrintLabelCopeNum>     <!--复制标签份数-->

  <ArogoxOutPutPort>Arogox</ArogoxOutPutPort>                        <!--立像打印机-->
  <ArogoxnEnable>1</ArogoxnEnable>                             <!--O定除eh境,1 -> 除eh境致能, 0 -> 除eh境P]-->
  <Arogoxhor>0</Arogoxhor>                                     <!--O定_始列印c,hor;水平界基c。挝 dot-->
  <Arogoxver>0</Arogoxver>                                     <!--O定_始列印c,ver: 垂直界基c。挝 dot-->
  <Arogoxobject>2</Arogoxobject>                               <!--印表C功能x-->
  <Arogoxdarkness>8</Arogoxdarkness>                           <!--O定岣蓄^列印岫龋：0～15，榷ㄊ 8-->
  <ArogoxIsImmediate>1</ArogoxIsImmediate>                     <!--用砹⒓①Y料魉统龌r入捍^中（1榱⒓魉, 只pbuf中的热葜凛出埠，0①Y料加到捍^後, 等呼叫B_Print_Out()r才魉停-->
  <Arogoxpbuf>nop_front\r\n</Arogoxpbuf>                       <!--用砹⒓①Y料魉统龌r入捍^中（要魉偷馁Y料指耍-->
  <ArogoxX>100</ArogoxX>                                       <!--X 座-->
  <ArogoxY>20</ArogoxY>                                        <!--Y 座-->
  <Arogoxori>0</Arogoxori>                                     <!--列印方向定位，'0'是 0°，'1'是90°、'2'是180°，'3'是270°-->
  <ArogoxEncodeType>1</ArogoxEncodeType>                       <!--条码类型-->
  <ArogoxBarNarrow>2</ArogoxBarNarrow>                         <!--条形码窄单元的宽度-->
  <ArogoxBarWide>3</ArogoxBarWide>                             <!--条形码宽单元的宽度-->
  <ArogoxBarcodeHeight>40</ArogoxBarcodeHeight>                <!--la高度-->
  <ArogoxPrintCode>N</ArogoxPrintCode>                         <!--打印码文( human 是'N'r，t不列印文字， human 是'B'r，t列印可x文字)-->
  <ArogoxBarCodeInterval>140</ArogoxBarCodeInterval>           <!--条形码之间的间距-->
  <ArogoxCodeInterval>100</ArogoxCodeInterval>                 <!--自定义码文与标签间隔-->
  <Arogoxfont>2</Arogoxfont>                                   <!--x褡中危1～5 x癯凶中-->
  <Arogoxhor_factor>1</Arogoxhor_factor>                       <!--水平放大比例，：1～24-->
  <Arogoxver_factor>1</Arogoxver_factor>                       <!--垂直放大比例，：1～24-->
  <Arogoxmode>N</Arogoxmode>                                   <!--反白功能，'N'普通文字，'R'反白文字-->
  <ArogoxPrintLabelCopeNum>1</ArogoxPrintLabelCopeNum>         <!--复制标签份数-->
</root>


